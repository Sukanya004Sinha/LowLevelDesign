package Stream;
interface AB{
    int add(int i, int j);
}
public class Demo {
    public static void main(String[] args){
        AB obj = new AB(){
            public int add(int i ,int j){
                return i+j;
            }
        };
        int result=obj.add (6 , 7);
        System.out.println("Result is " +result);
    }
}
